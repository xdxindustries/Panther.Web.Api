---
swagger: "2.0"
info:
  description: Panther Systems Web API
  version: "0.1"
  title: Panther.Web.Api
  contact:
    name: Steve Smith
    email: ssmith@panthersys.com
host: virtserver.swaggerhub.com
basePath: /xdxindustries/Panther.Web.Api/0.1
tags:
- name: Orders
  description: A order for the mill
  externalDocs:
    description: tbd documenation at
    url: http://panthersys.com
- name: Customers
  description: Customers who buy orders
  externalDocs:
    description: tbd documenation at
    url: http://panthersys.com
- name: Shipments
  description: Shipments of orders
  externalDocs:
    description: tbd documenation at
    url: http://panthersys.com
schemes:
- https
security:
- basicAuth: []
paths:
  /api/Orders:
    get:
      tags:
      - Orders
      summary: Gets a list of all orders in the system
      parameters: []
      responses:
        200:
          description: List of orders
          schema:
            type: array
            items:
              $ref: '#/definitions/Order'
        401:
          description: Unauthorized
    post:
      tags:
      - Orders
      summary: Creates a new order
      consumes:
      - application/json
      parameters:
      - in: body
        name: body
        description: The serialized JSON of the order to create
        required: true
        schema:
          $ref: '#/definitions/Order'
      responses:
        200:
          description: OK
  /api/Orders/{orderKey}:
    get:
      tags:
      - Orders
      summary: Returns an order by Key.
      parameters:
      - name: orderKey
        in: path
        required: true
        type: number
      responses:
        200:
          description: An order
          schema:
            $ref: '#/definitions/Order'
        401:
          description: Unauthorized
        404:
          description: Not Found
  /api/Customers:
    get:
      tags:
      - Customers
      summary: Gets list of acceptable customers
      parameters: []
      responses:
        200:
          description: List of customers
          schema:
            type: array
            items:
              $ref: '#/definitions/ShipTo'
        401:
          description: Unauthorized
    post:
      tags:
      - Customers
      summary: Creates a new customer
      consumes:
      - application/json
      parameters:
      - in: body
        name: body
        description: The serialized JSON of the customer to create
        required: true
        schema:
          $ref: '#/definitions/ShipTo'
      responses:
        200:
          description: OK
  /api/Customers/{ShipToKey}:
    get:
      tags:
      - Customers
      summary: Returns a Customer by ShipToKey.
      parameters:
      - name: ShipToKey
        in: path
        description: The ShipToKey of the customer to return.
        required: true
        type: number
      responses:
        200:
          description: A customer
          schema:
            $ref: '#/definitions/ShipTo'
        401:
          description: Unauthorized
        404:
          description: Not Found
  /api/Shipments:
    get:
      tags:
      - Shipments
      summary: Gets list of shipments
      parameters: []
      responses:
        200:
          description: List of Shipments
          schema:
            type: array
            items:
              $ref: '#/definitions/Manifest'
        401:
          description: Unauthorized
  /api/Deliveries:
    get:
      tags:
      - Deliveries
      summary: Gets a list of all pending deliveries in the system
      parameters: []
      responses:
        200:
          description: List of pending deliveries
          schema:
            type: array
            items:
              $ref: '#/definitions/Delivery'
        401:
          description: Unauthorized
    post:
      tags:
      - Deliveries
      summary: Creates a new delivery
      consumes:
      - application/json
      parameters:
      - in: body
        name: body
        description: The serialized JSON of the delivery to create
        required: true
        schema:
          $ref: '#/definitions/Delivery'
      responses:
        200:
          description: OK
          schema:
            $ref: '#/definitions/Delivery'
securityDefinitions:
  basicAuth:
