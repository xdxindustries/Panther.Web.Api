swagger: "2.0"
info:
  description: Panther Systems Web API
  version: "0.1"
  title: Panther.Web.Api
host: virtserver.swaggerhub.com
basePath: /xdxindustries/Panther.Web.Api/0.1
schemes:
- https
security:
- basicAuth: []
paths:
  /api/Orders:
    get:
      tags:
      - Orders
      summary: Gets a list of all orders in the system
      parameters: []
      responses:
        200:
          description: List of orders
          schema:
            type: array
            items:
              $ref: '#/definitions/Order'
        401:
          description: Unauthorized
  /api/Orders/{orderKey}:
    get:
      tags:
      - Orders
      summary: Returns an order by Key.
      parameters:
      - name: orderKey
        in: path
        required: true
        type: number
      responses:
        200:
          description: An order
          schema:
            $ref: '#/definitions/Order'
        401:
          description: Unauthorized
        404:
          description: Not Found
  /api/Customers:
    get:
      tags:
      - Customers
      summary: Gets list of acceptable customers
      parameters: []
      responses:
        200:
          description: List of customers
          schema:
            type: array
            items:
              $ref: '#/definitions/ShipTo'
        401:
          description: Unauthorized
  /api/Customers/{ShipToKey}:
    get:
      tags:
      - Customers
      summary: Returns a Customer by ShipToKey.
      parameters:
      - name: ShipToKey
        in: path
        description: The ShipToKey of the customer to return.
        required: true
        type: number
      responses:
        200:
          description: A customer
          schema:
            $ref: '#/definitions/ShipTo'
        401:
          description: Unauthorized
        404:
          description: Not Found
